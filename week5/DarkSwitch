import { HStack, Switch, useColorMode } from "@chakra-ui/react";

function DarkSwitch(){
    let {colorMode,toggleColorMode}=useColorMode();
    return(
    
    <HStack>
      <Switch colorScheme="pink" isChecked={colorMode=='dark'}  onChange={toggleColorMode}>Darkswitch</Switch>
    </HStack>
    )

}
export default DarkSwitch;
